<view class="table">
  <view class="tr bg-w">
    <view class="th">姓名</view>
    <view class="th">日志</view>
  </view>
	<block wx:for="{{listData}}" wx:key="{{code}}" 
	wx:if="{{weidu=='true'}}" >
    <view class="tr bg-g" wx:if="{{item.type == true}}">
      <view class="td">{{item.code}}</view>
      <view class="td" bindtap="detail">{{item.text}}</view>
  	</view>
	</block>

	<block wx:for="{{listData}}" wx:key="{{code}}" 
	wx:if="{{quanbu=='true'}}" >
    <view class="tr bg-g">
      <view class="td">{{item.code}}</view>
      <view class="td" bindtap="detail">{{item.text}}</view>
  	</view>
	</block>
</view>
